{
  "packages": {
    ".": {
      "release-type": "rust",
      "include-v-in-tag": true,
      "include-component-in-tag": false,
      "bump-patch-for-minor-pre-major": true,
      "extra-files": [
        {
          "type": "generic",
          "path": "README.md",
          "search": "version: v\\d+\\.\\d+\\.\\d+",
          "replace": "version: v{{version}}"
        }
      ]
    }
  }
}
